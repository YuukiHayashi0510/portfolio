{
  "name": "next.ts.prisma-fullstack-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt": "prettier --write \"app/**/*.{ts,tsx}\" \"lib/**/*.ts\" \"tests/**/*.ts\" \"types/**/*.ts\"",
    "fmt:check": "prettier --check \"app/**/*.{ts,tsx}\" \"lib/**/*.ts\" \"tests/**/*.ts\" \"types/**/*.ts\"",
    "db:generate": "yarn prisma generate && dotenv -e .env.test -- yarn prisma generate",
    "db:migrate": "yarn prisma migrate dev --skip-seed",
    "db:migrate:test": "dotenv -e .env.test -- yarn prisma migrate dev --skip-seed",
    "db:reset": "yarn prisma migrate reset --skip-seed",
    "db:seed": "yarn prisma db seed",
    "db:studio": "yarn prisma studio",
    "db:studio:test": "dotenv -e .env.test -- yarn prisma studio",
    "erd": "prisma generate",
    "test": "dotenv -e .env.test -- yarn jest --runInBand --silent",
    "test:ci": "yarn jest --runInBand --silent",
    "test:watch": "dotenv -e .env.test -- yarn jest --watch --runInBand",
    "storage:init": "docker compose build && docker compose up -d",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o ./public/storybook"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "axios": "^1.5.0",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@mermaid-js/mermaid-cli": "^10.4.0",
    "@prisma/generator-helper": "^5.3.1",
    "@storybook/addon-essentials": "^7.4.2",
    "@storybook/addon-interactions": "^7.4.2",
    "@storybook/addon-links": "^7.4.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.2",
    "@storybook/nextjs": "^7.4.2",
    "@storybook/react": "^7.4.2",
    "@storybook/testing-library": "^0.2.1",
    "@types/jest": "^29.5.5",
    "@types/jsdom": "^21.1.3",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "autoprefixer": "10.4.15",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "postcss": "8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.3.1",
    "prisma-erd-generator": "^1.11.1",
    "storybook": "^7.4.2",
    "tailwindcss": "3.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  }
}
